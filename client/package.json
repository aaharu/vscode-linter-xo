{
	"engines": {
		"vscode": "^1.58.0"
	},
	"scripts": {
		"lint": "node ../node_modules/xo/cli.js"
	},
	"dependencies": {
		"vscode-languageclient": "^7.0.0"
	},
	"xo": {
		"ignores": [
			"!out/**"
		],
		"rules": {
			"@typescript-eslint/no-namespace": "warn",
			"unicorn/prefer-node-protocol": "off",
			"unicorn/prevent-abbreviations": "warn"
		}
	}
}
